{
  "name": "factorial-components",
  "version": "1.0.0",
  "description": "library of factorial components",
  "main": "index.js",
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "npm run clean && webpack --config webpack/server.config.js && webpack --config webpack/client.config.js",
    "format": "prettier-standard --print-width 60 \"src/**/*.js\"",
    "clean": "rimraf dist",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/factorialco/factorial-components.git"
  },
  "author": "Factorial",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/factorialco/factorial-components/issues"
  },
  "homepage": "https://github.com/factorialco/factorial-components#readme",
  "devDependencies": {
    "@storybook/react": "^3.0.1",
    "autoprefixer": "7.1.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-tcomb": "0.3.25",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-for-of": "6.23.0",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-node": "5.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "0.11.1",
    "flow-bin": "0.47.0",
    "husky": "0.13.4",
    "lint-staged": "3.6.0",
    "node-sass": "4.5.3",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "prettier-standard": "5.0.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.1",
    "tcomb": "3.2.20",
    "tcomb-react": "0.9.3",
    "webpack": "2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "autobind-decorator": "1.4.1",
    "classnames": "^2.2.5",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router": "3.0.5"
  }
}
